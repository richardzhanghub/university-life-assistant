<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <variable
            name="eventTimeEntry"
            type="com.cs446.group18.timetracker.entity.TimeEntry"/>
    </data>

    <androidx.cardview.widget.CardView
        android:id="@+id/viewTimeEntryContent"
        android:clickable="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="3dp">

        <RelativeLayout
            android:id="@+id/time_entry_list_item_layout"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:padding="8dp">

            <TextView
                android:id="@+id/time_entry_startTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Text1"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/time_entry_endTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Text2"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/time_entry_duration"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Text2"
                android:textSize="20sp" />
        </RelativeLayout>

    </androidx.cardview.widget.CardView>
</layout>